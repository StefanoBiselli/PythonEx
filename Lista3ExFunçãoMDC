def mdc(num1,num2):
    if num1 > num2:
        maior = num1
    else:
        maior = num2
    while True:
        if num1 % maior == 0 and num2 % maior == 0:
            return maior
            break
        else:
            maior = maior - 1

num1, num2 = int(input("Digite um número inteiro: ")), int(input("Digite outro número inteiro: "))
print("O maior divisor comum é:", mdc(num1,num2))
