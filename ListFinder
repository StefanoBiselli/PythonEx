import random
def generateList(n):
  lista = []
  for i in range (n):
    lista.append(random.randint(1,10))
  return lista

def linearSearch(lista, num):
  n = len(lista)
  for i in range(n-1):
    if lista[i] == num:
      return i
  return -1


listas = generateList(10)
print(generateList(10))
print(linearSearch(listas, 10))
